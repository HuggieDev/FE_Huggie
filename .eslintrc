{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": "warn",
    "semi": "error",
    "no-undef": "error",
    "indent": ["error", 2],
    "no-trailing-spaces": "error",
    "quotes": ["error","single"],
    "comma-dangle": ["error", "always-multiline"],
    "no-var": "error",
    "arrow-parens": ["error"]
  }
}